<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_math_game"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="wakeup.wakeupapp.MathGame">

    <Button
        android:id="@+id/expression4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Blank"
        android:layout_below="@+id/expression3"
        android:layout_alignStart="@+id/expression3"
        android:layout_marginTop="56dp" />

    <Button
        android:id="@+id/expression3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Blank"
        android:layout_marginTop="51dp"
        android:layout_below="@+id/expression2"
        android:layout_alignStart="@+id/expression2" />

    <Button
        android:id="@+id/expression1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Blank"
        android:layout_marginTop="41dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/expression2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Blank"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/expression1"
        android:layout_alignStart="@+id/expression1" />

</RelativeLayout>
